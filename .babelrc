{
  "presets": [
    "react",
    "es2015",
    "stage-0",
    [ "env", {
      "targets": {
        "browsers": "> 2%"
      }
    }]
  ],
  "plugins": [
    "react-hot-loader/babel",
    "transform-class-properties",
    "transform-object-rest-spread",
    "syntax-decorators",
    "array-includes",
    "transform-decorators-legacy",
    "transform-object-assign"
  ]
}
